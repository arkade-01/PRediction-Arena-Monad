generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Agent {
  id           String   @id @default(uuid())
  name         String
  ticker       String
  address      String   @unique
  privateKey   String
  ownerAddress String
  tokenAddress String?
  strategy     String   @default("Technical Analysis")
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
